PatchScript: <<
# Move version file cause it'll get included from Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include/c++/v1/cmath:309  SOOOOOOOOOoo dumb
mv VERSION VERSION.txt
perl -pi -e 's,CMAKE_SOURCE_DIR[\}][\/]VERSION,CMAKE_SOURCE_DIR}/VERSION.txt,g' cmake/mysql_version.cmake
mv storage/mroonga/version storage/mroonga/version.txt
perl -pi -e 's,MRN_SOURCE_DIR[\}][\/]version    ,MRN_SOURCE_DIR}/version.txt,g' storage/mroonga/CMakeLists.txt

# Fix iConv
perl -pi -e 's,-DLIBICONV_PLUG,-DNO_LIBICONV_PLUG,g' libmariadb/libmariadb/CMakeLists.txt

# Fix run dir location
for i in `find debian -type f`; do perl -pi -e 's, /run/mysqld, /var/run/mysqld,g' $i; done

dh_quilt_patch

%{default_script}

perl -pi -e 's,..DEB_HOST_GNU_TYPE..,/usr/bin/,g' debian/rules
perl -pi -e 's,lsb_release -r -s,echo macOS,g' debian/rules
perl -pi -e 's,"debian-,"fink-,g' debian/rules
perl -pi -e 's,amd64,darwin-x86_64,g' debian/rules
perl -pi -e 's,kfreebsd-darwin-x86_64,kfreebsd-amd64,g' debian/rules
perl -pi -e 's,install -D,ginstall -D,g' debian/rules

### Apple Silicon fixes
perl -pi -e 's,defined.__x86_64__.,defined(__x86_64__) || defined(__arm64__),g' include/my_global.h

for i in `find debian -type f`; do \
	perl -pi -e 's,libmariadb.so([0-9.\*]+)?,libmariadb$1.dylib,g' $i; \
	perl -pi -e 's,libmariadbd.so([0-9.\*]+)?,libmariadbd$1.dylib,g' $i; \
	perl -pi -e 's,libmysqlclient.so([0-9.\*]+)?,libmysqlclient$1.dylib,g' $i; \
	perl -pi -e 's,libmysqlclient_r.so([0-9.\*]+)?,libmysqlclient_r$1.dylib,g' $i; \
done

# Can't build these yet, missing depends
rm -rf debian/*oqgraph* debian/*crack* debian/*rocksdb* debian/*tokudb* debian/*mroonga*

# Remove AppArmor
sed -i '/apparmor/d' debian/mariadb-server-10.3.install
sed -i '/apparmor/d' debian/rules
sed -i '/AppArmor/d' debian/rules

# Remove add/del user/group stuffs
sed -i '123,142d' debian/mariadb-server-10.3.preinst
sed -i '47d;' debian/mariadb-server-10.3.postrm

# GNU tools
perl -pi -e 's,df --portability,gdf --portability,g' debian/mariadb-server-10.3.mysql.init debian/mariadb-server-10.3.preinst
perl -pi -e 's,rmdir,grmdir,g' debian/mariadb-server-10.3.postrm

# User _mysql instead of mysql user/group
perl -pi -e 's,mysql:adm,_mysql:admin,g' debian/mariadb-server-10.3.postinst
perl -pi -e 's,--user=mysql,--user=_mysql,g' debian/mariadb-server-10.3.postinst
perl -pi -e 's,chown mysql,chown _mysql,g' debian/mariadb-server-10.3.postinst debian/mariadb-server-10.3.preinst
perl -pi -e 's,-group mysql,-group _mysql,g' debian/mariadb-server-10.3.preinst
perl -pi -e 's,chgrp mysql,chgrp _mysql,g' debian/mariadb-server-10.3.preinst
perl -pi -e 's,id -u mysql,id -u _mysql,g' debian/mariadb-server-10.3.postinst debian/mariadb-server-10.3.preinst
perl -pi -e 's,mysql adm,_mysql admin,g' debian/mariadb-server-10.3.mysql-server.logrotate
perl -pi -e 's,-o mysql,-o _mysql,g' debian/mariadb-server-10.3.mysql.init
perl -pi -e 's,= mysql,= _mysql,g' debian/additions/mariadb.conf.d/50-server.cnf
perl -pi -e "s,user='mysql',user='mysql',g" scripts/mysqld_safe.sh

# Fix init script
perl -pi -e 's,-g root,-g wheel,g' debian/mariadb-server-10.3.mysql.init
perl -pi -e 's,/lib/lsb,%p/lib/lsb,g' debian/mariadb-server-10.3.mysql.init

# no invoke-rc on macOS
perl -pi -e 's,invoke-rc.d mysql start,daemonic enable mariadb || exit \$?\n\t\t%p/etc/init.d/mysql start,g' debian/mariadb-server-10.3.postinst
perl -pi -e 's,invoke-rc.d mysql stop,%p/etc/init.d/mysql stop || exit \$?\n\tdaemonic disable mariadb,g' debian/mariadb-server-10.3.prerm

# Fix files not using the build system DEFINES
perl -pi -e 's,/etc,%p/etc,g' mysys/my_default.c

# Fix cmake file
perl -pi -e 's,END[\(][\)],ENDIF(),g' libmariadb/cmake/ConnectorName.cmake
