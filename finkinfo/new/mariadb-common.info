CMAKEFLAGS="-Wno-dev -DICONV_INCLUDE_DIR:PATH=%p/include -DICONV_LIBRARIES:FILEPATH=%p/lib/libiconv.dylib -Dreadline_INCLUDE_DIR:PATH=%p/include/readline -Dreadline_LIBRARY:FILEPATH=%p/lib/libreadline.dylib CURL_INCLUDE_DIR:PATH=%p/include -DCURL_LIBRARY:FILEPATH=%p/lib/libcurl.dylib -DCURSES_CURSES_H_PATH:PATH=%p/include -DCURSES_CURSES_LIBRARY:FILEPATH=%p/lib/libcurses.dylib -DCURSES_HAVE_CURSES_H:FILEPATH=%p/include/curses.h -DCURSES_INCLUDE_PATH:FILEPATH=%p/include -DCURSES_LIBRARY:FILEPATH=%p/lib/libcurses.dylib -DCURSES_NCURSES_LIBRARY:FILEPATH=%p/lib/libncurses.dylib -DINSTALL_MYSQLDATADIR:STRING=%p/var/lib/mysql -DINSTALL_SYSCONF2DIR:STRING=%p/etc/mysql/conf.d -DINSTALL_SYSCONFDIR:STRING=%p/etc -DINSTALL_SYSTEMD_SYSUSERSDIR:STRING=%p/lib/sysusers.d -DINSTALL_SYSTEMD_TMPFILESDIR:STRING=%p/lib/tmpfiles.d -DINSTALL_SYSTEMD_UNITDIR:STRING=%p/lib/systemd/system -DINSTALL_UNIX_ADDRDIR:STRING=%p/var/run/mysqld/mysqld.sock -DMYSQL_DATADIR:PATH=%p/var/lib/mysql -DBZIP2_INCLUDE_DIR:PATH=%p/include -DBZIP2_LIBRARIES:FILEPATH=%p/lib/libbz2.dylib -DWITH_LIBEDIT=OFF -DCMAKE_BUILD_WITH_INSTALL_RPATH=ON" %{default_script}
